# tags are used for organizing operations
tags:
- name: admins
  description: Secured Admin-only calls
- name: developers
  description: Operations available to regular developers

paths:
  /upcoming_events:
    get:
      tags:
      - developers
      summary: searches inventory
      operationId: searchInventory
      description: |
        Displays list of events recommended for the user
      produces:
      - application/json
      parameters:
      - in: query
        name: userId
        description: pass user ID
        required: true
        type: string
      responses:
        200:
          description: event recommendations for
          schema:
            type: array
            items:
              $ref: '#/definitions/UpcomingEventsResponse'
        403:
          description: Unauthorized
          schema:
            $ref: '#/definitions/Error'
        500:
          description: Unexpected error
          schema:
            $ref: '#/definitions/Error'
definitions:
  UpcomingEventsResponse:
    type: object
    properties:
      messages:
        type: array
        items:
          $ref: '#/definitions/Event'
  Event:
    type: object
    properties:
      id:
        type: string
      summary:
        type: string
      location:
        type: string
  Error:
    type: object
    properties:
      code:
        type: integer
        format: int32
      message:
        type: string
